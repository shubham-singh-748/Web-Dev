alert('What is your age')
a = prompt('Enter here')
a=Number.parseInt(a)
const isEligible=(a)=>{
  return a>=18?true:false
}
if(isEligible(a)) {
  alert("Of couse he/she can drive")
}
else{
  alert("sorry, not eligible")
}

